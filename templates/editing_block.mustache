{{!
    @template format_page/editing_block

    Classes required for JS:
    * none

}}
<!-- format_page/editing_block -->
<div id="format-page-editing-block">

    {{{tabs}}}

    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span4 col-md-4">
                {{#str}}setcurrentpage, format_page{{/str}}
                {{{jumpmenu}}}
            </div>

            {{#canedit}}
            <div class="span4 col-md-4">
                {{{addmodmenu}}}
            </div>

            <div class="span4 col-md-4">
                {{{addmenus}}}
            </div>
            {{/canedit}}
        </div>

        <div class="row-fluid"></div>
    </div>
</div>
<!-- /format_page/editing_block -->
